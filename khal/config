[calendars]
    [[0]]
        type = discover
        path = ~/Calendars/*
        color = "#FDF6E3"

[default]
default_calendar = "Events"
highlight_event_days = True

[highlight_days]
color = "#EEE8D5"

[locale]
dateformat = %m/%d
timeformat = %I:%M %p
longdateformat = %m/%d/%Y
datetimeformat = %m/%d %I:%M %p
longdatetimeformat = %m/%d/%Y %I:%M %p
firstweekday = 6
